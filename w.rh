        copy_packets(
            ws_decoder(#{}, read_chunk_limiter(take_read_part(s2u), 13)),
            //trivial_pkts(),
            ws_encoder(#{masked: true}, take_write_part(s2u)),
            //display_pkts(),
        )
    })
})
